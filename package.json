{
  "name": "use-aragon",
  "version": "0.0.0",
  "description": "React hooks for Aragon OSx",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/pythonpete32/use-aragon"
  },
  "scripts": {
    "prepublish": "tsc",
    "format": "prettier --check 'src/**/*.ts*'",
    "format:write": "prettier --write 'src/**/*.ts*'",
    "lint": "eslint 'src/**/*.ts*'",
    "lint:fix": "eslint --fix 'src/**/*.ts*'",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts*": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "@aragon/sdk-client": "0.22.0-beta",
    "react-query": "^3.39.3",
    "ethers": "^5.7.2",
    "wagmi": "~0.11.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.17",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard-with-typescript": "^22.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.2.5",
    "eslint-plugin-promise": "^6.0.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.8.3"
  },
  "author": "Aaron Abu Usama <Aaron@DAOresear.ch>",
  "license": "MIT"
}